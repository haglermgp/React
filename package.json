{
  "name": "ultra-jockey",
  "version": "1.0.0",
  "description": "Online Game of bets",
  "main": "index.js",
  "repository": "https://github.com/mayupi/UltraJockey",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build:redirects && parcel build ./public/index.html --out-dir build/ --public-url ./",
    "build:redirects": "mkdir build && echo '/* /index.html 200' > build/_redirects",
    "clean": "rimraf build/ .cache/ dist/",
    "format": "prettier-standard 'src/**/*.js'",
    "lint": "standard",
    "start": "parcel ./public/index.html --no-cache"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "emotion": "^9.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-eslint": "7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-emotion": "^9.0.1",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-boilerplate": "^1.1.1",
    "cross-env": "^5.1.3",
    "husky": "^0.15.0-rc.8",
    "lint-staged": "^7.0.0",
    "parcel-bundler": "^1.5.1",
    "prettier-standard": "^8.0.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  }
}
